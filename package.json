{
  "name": "ts-swarm",
  "version": "1.0.5",
  "description": "A minimal TypeScript Agentic library inspired by the OpenAI Swarm API.",
  "keywords": [
    "swarm",
    "ai",
    "agentic",
    "typescript",
    "openai",
    "agent",
    "agentic-workflow"
  ],
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "ci": "pnpm build && pnpm check:format && pnpm check:types && pnpm lint && pnpm test",
    "lint": "tsc",
    "format": "prettier --write .",
    "check:format": "prettier --check ./src",
    "check:types": "attw --pack . --ignore-rules=cjs-resolves-to-esm",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "changeset": "changeset",
    "release": "changeset version && changeset publish",
    "ci:release": "pnpm build && pnpm release",
    "prepublishOnly": "pnpm run ci",
    "example:basic": "npx tsx examples/basic/main.ts",
    "example:weather": "npx tsx examples/weather/main.ts",
    "example:custom": "npx tsx examples/custom/main.ts",
    "ai:triage-weather-email": "npx tsx src/ai/test.ts",
    "ai:pokemon": "npx tsx src/ai/example/pokemon.ts",
    "ai:filesystem": "npx tsx src/ai/example/filesystem.ts"
  },
  "dependencies": {
    "@ai-sdk/openai": "^0.0.70",
    "ai": "^3.4.20",
    "mathjs": "^13.2.0",
    "openai": "^4.57.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.16.4",
    "@changesets/cli": "^2.27.9",
    "@types/node": "^22.7.9",
    "@vitest/coverage-v8": "^2.1.3",
    "babel-eslint": "^10.1.0",
    "dotenv": "^16.4.5",
    "prettier": "^3.3.3",
    "tsup": "^8.3.4",
    "typescript": "^5.6.3",
    "vitest": "^2.1.3"
  },
  "homepage": "https://github.com/joshmu/ts-swarm",
  "bugs": {
    "url": "https://github.com/joshmu/ts-swarm/issues"
  },
  "author": "Josh Mu <hello@joshmu.dev> (https://joshmu.dev)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joshmu/ts-swarm.git"
  },
  "license": "MIT"
}