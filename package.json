{
  "name": "ts-swarm",
  "version": "1.0.0",
  "description": "A minimal TypeScript Agentic library inspired by the OpenAI Swarm API.",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "ci": "pnpm build && pnpm check:format && pnpm test",
    "format": "prettier --write .",
    "check:format": "prettier --check .",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "example:basic": "npx tsx src/examples/basic/main.ts",
    "example:custom": "npx tsx src/examples/custom/main.ts"
  },
  "keywords": [
    "swarm",
    "ai",
    "agentic",
    "typescript",
    "openai",
    "agent",
    "agentic-workflow"
  ],
  "homepage": "https://github.com/joshmu/ts-swarm",
  "bugs": {
    "url": "https://github.com/joshmu/ts-swarm/issues"
  },
  "author": "Josh Mu <hello@joshmu.dev> (https://joshmu.dev)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joshmu/ts-swarm.git"
  },
  "license": "MIT",
  "dependencies": {
    "openai": "^4.57.0"
  },
  "devDependencies": {
    "@types/node": "^22.7.9",
    "@vitest/coverage-v8": "^2.1.3",
    "babel-eslint": "^10.1.0",
    "dotenv": "^16.4.5",
    "prettier": "^3.3.3",
    "typescript": "^5.6.3",
    "vitest": "^2.1.3"
  },
  "type": "module",
  "files": [
    "dist"
  ]
}
